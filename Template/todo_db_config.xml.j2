<Context>
  <Resource name="jdbc/todoDB"
            auth="Container"
            type="javax.sql.DataSource"
            maxTotal="100"
            maxIdle="30"
            maxWaitMillis="10000"
            username="{{ db_user }}"
            password="{{ db_password }}"
            driverClassName="com.mysql.jdbc.Driver"
            url="jdbc:mysql://{{ db_host }}:3306/DatabaseTSL"/>
</Context>
